{
  "name": "@swim-io/pool-utils",
  "version": "0.1.1",
  "description": "A package for common pool interfaces and types.",
  "main": "build",
  "types": "types",
  "files": [
    "build/",
    "types/",
    "*.md",
    "!*.test.*",
    "!**/fixtures/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swim-io/swim/tree/master/packages/pool-utils"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "typecheck": "tsc",
    "format": "prettier --write \"./src/**/*.ts\"",
    "format:check": "prettier --check \"./src/**/*.ts\"",
    "lint": "eslint \"./src/**/*.ts\"",
    "lint:fix": "eslint --fix \"./src/**/*.ts\"",
    "test": "jest",
    "verify": "yarn typecheck && yarn format:check && yarn lint && yarn test --watchAll=false",
    "build": "rm -rf ./build/ ./types/ && tsc",
    "prepare": "yarn verify && yarn build"
  },
  "dependencies": {
    "@project-serum/borsh": "^0.2.2",
    "@solana/spl-token": "^0.1.8",
    "@solana/web3.js": "^1.33.0",
    "@types/buffers": "^0.1.31",
    "bn.js": "^5.2.0",
    "decimal.js": "^10.3.1",
    "react-scripts": "4.0.3"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.15",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "buffer": "^6.0.3",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-functional": "^4.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^26.4.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-storybook": "^0.5.12",
    "jest": "^28.1.1",
    "jest-mock-extended": "^2.0.5",
    "prettier": "^2.3.2",
    "ts-jest": "^28.0.5",
    "typechain": "^5.1.2",
    "typescript": "~4.2",
    "webpack": "4.44.2"
  }
}
