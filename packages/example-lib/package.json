{
  "name": "@swim/example-lib",
  "version": "0.0.0",
  "main": "build",
  "types": "types",
  "files": [
    "build/",
    "types/",
    "*.md",
    "!*.test.*",
    "!**/fixtures/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/swim-io/swim-ts-monorepo/tree/master/packages/example-lib"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "format": "prettier --write \"./src/**/*.ts\"",
    "build": "rm -rf ./build/ ./types/ && tsc"
  },
  "devDependencies": {
    "prettier": "^2.6.1",
    "typescript": "^4.6.3"
  }
}
